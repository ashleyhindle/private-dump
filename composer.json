{
    "name": "ashleyhindle/private-dump",
    "description": "CLI script to dump your MySQL database, and sanitise the output of sensitive data for development/staging use",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Ashley Hindle",
            "email": "hey@ashleyhindle.com"
        }
    ],
    "bin": ["bin/private-dump"],
    "require": {
        "php": "^5.6||^7.0",
        "ext-json": "*",
        "nategood/commando": "^0.3.0",
        "ifsnop/mysqldump-php": "dev-feature/allow-where-per-table",
        "fzaninotto/faker": "^1.8",
        "dflydev/dot-access-data": "^1.0"
    },

    "require-dev": {
        "phpunit/phpunit": "5.7.27"
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:ashleyhindle/mysqldump-php.git"
        }
    ],
    "autoload": {
        "psr-0": {
            "PrivateDump": "src/PrivateDump"
        }
    },
    "config": {
        "platform": {
            "php": "5.6.1"
        }
    }
}
